{
  "name": "List milestones for a repository",
  "enabledForApps": true,
  "method": "GET",
  "path": "/repos/:owner/:repo/milestones",
  "params": [
    {
      "name": "owner",
      "type": "string",
      "required": true,
      "description": ""
    },
    {
      "name": "repo",
      "type": "string",
      "required": true,
      "description": ""
    },
    {
      "name": "state",
      "type": "enum",
      "description": "The state of the milestone. Either `open`, `closed`, or `all`.",
      "default": "open",
      "required": false,
      "options": [
        "open",
        "closed",
        "all"
      ]
    },
    {
      "name": "sort",
      "type": "enum",
      "description": "What to sort results by. Either `due_on` or `completeness`.",
      "default": "due_on",
      "required": false,
      "options": [
        "due_on",
        "completeness"
      ]
    },
    {
      "name": "direction",
      "type": "enum",
      "description": "The direction of the sort. Either `asc` or `desc`.",
      "default": "asc",
      "required": false,
      "options": [
        "asc",
        "desc"
      ]
    },
    {
      "name": "per_page",
      "type": "number",
      "required": false,
      "description": "Results per page (max 100)",
      "default": 30
    },
    {
      "name": "page",
      "type": "number",
      "required": false,
      "description": "Page number of the results to fetch.",
      "default": 1
    }
  ],
  "description": "",
  "documentationUrl": "https://developer.github.com/v3/issues/milestones/#list-milestones-for-a-repository"
}
