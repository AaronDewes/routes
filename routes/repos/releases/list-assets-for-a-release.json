{
  "name": "List assets for a release",
  "enabledForApps": true,
  "method": "GET",
  "path": "/repos/:owner/:repo/releases/:id/assets",
  "params": [
    {
      "name": "owner",
      "type": "string",
      "required": true,
      "description": "",
      "location": "path"
    },
    {
      "name": "repo",
      "type": "string",
      "required": true,
      "description": "",
      "location": "path"
    },
    {
      "name": "id",
      "type": "string",
      "required": true,
      "description": "",
      "location": "path"
    },
    {
      "name": "per_page",
      "type": "integer",
      "required": false,
      "description": "Results per page (max 100)",
      "default": 30,
      "location": "query"
    },
    {
      "name": "page",
      "type": "integer",
      "required": false,
      "description": "Page number of the results to fetch.",
      "default": 1,
      "location": "query"
    }
  ],
  "description": "",
  "documentationUrl": "https://developer.github.com/v3/repos/releases/#list-assets-for-a-release"
}
